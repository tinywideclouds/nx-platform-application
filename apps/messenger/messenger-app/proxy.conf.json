{
  "/api/auth": {
    "target": "http://localhost:3000",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true
  },
  "/api/contacts": {
    "target": "http://localhost:3001",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true
  },
  "/api/routing": {
    "target": "http://localhost:8082",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true,
    "pathRewrite": {
      "^/api/routing": "/api"
    }
  },
  "/api/keys": {
    "target": "http://localhost:8081",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true,
    "pathRewrite": {
      "^/api/keys": "/keys"
    }
  },
  "/api/notifications": {
    "target": "http://localhost:8083",
    "secure": false,
    "logLevel": "debug",
    "changeOrigin": true,
    "pathRewrite": {
      "^/api/notifications": "/notifications"
    }
  },
  "/connect": {
    "target": "ws://localhost:8083",
    "secure": false,
    "ws": true,
    "logLevel": "debug",
    "changeOrigin": true
  }
}
