{
  "name": "messenger-infrastructure-key-storage",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/messenger/infrastructure/-storage/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": [
    "scope:messenger",
    "layer:infrastructure"
  ],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": [
        "{options.outputPath}",
        "{options.outputHtmlPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/libs/messenger/infrastructure/key-storage",
        "outputHtmlPath": "dist/test-reports/libs/messenger/infrastructure/key-storage/index.html",
        "configFile": "libs/messenger/infrastructure/key-storage/vite.config.mts"
      }
    },
    "test": {
      "executor": "@nx/vitest:test",
      "outputs": [
        "{options.reportsDirectory}"
      ],
      "options": {
        "reportsDirectory": "coverage/libs/messenger/infrastructure/key-storage",
        "configFile": "libs/messenger/infrastructure/key-storage/vitest.config.mts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
